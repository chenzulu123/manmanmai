* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
a {
  text-decoration: none;
  color: inherit;
  display: block;
  width: 100%;
  height: 100%;
  /* 解决点击高亮*/
  -webkit-tap-highlight-color: transparent;
}
input {
  display: block;
  width: 100%;
  height: 100%;
  border: none;
}
img {
  display: block;
  width: 100%;
}
ul {
  list-style: none;
}
.clearfix:after {
  display: block;
  content: "";
  height: 0;
  clear: both;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
/*导入基础样式*/
/*最外层的大盒子*/
html,
body {
  height: 100%;
  width: 100%;
}
.qcj_cabbagePrice {
  padding: 82px 0;
  background-color: #f7f7f7;
  height: 100%;
  width: 100%;
}
/*白菜价头部 begin---------------------*/
.cabbage_head {
  width: 100%;
  height: 44px;
  background-color: #e4393c;
  padding: 0 50px;
  /*挤出位置放左右两个图标*/
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  /* 左边箭头*/
  /*右边图片*/
}
.cabbage_head a {
  height: 100%;
  width: 50px;
  position: absolute;
}
.cabbage_head .prev {
  top: 0;
  left: 0;
  padding: 10px 10px;
  color: white;
}
.cabbage_head h1 {
  margin: 0;
  color: white;
  font-size: 20px;
  font-weight: bold;
  text-align: center;
  line-height: 44px;
}
.cabbage_head .downLoad {
  position: absolute;
  right: 0px;
  top: 0px;
  padding: 5px 10px;
}
.cabbage_head .downLoad span {
  display: block;
  width: 34px;
  height: 34px;
  background-size: 34px 34px;
  background-image: url('../images/header_app.png');
}
/*白菜价头部 end---------------------*/
/*白菜价tab begin---------------------*/
#wrapper {
  position: absolute;
  top: 44px;
  width: 100%;
  height: 38px;
  overflow: hidden;
  z-index: 10;
}
#wrapper #scroller {
  width: 185%;
  height: 38px;
  position: absolute;
}
#wrapper #scroller .cabbage_tab {
  width: 100%;
  height: 38px;
  background-color: #fff;
  overflow: hidden;
  border-bottom: 1px solid #ccc;
  margin-bottom: 6px;
  /* 禁用浏览器默认的触摸行为  */
}
#wrapper #scroller .cabbage_tab ul {
  width: 200%;
  height: 38px;
}
#wrapper #scroller .cabbage_tab ul li {
  float: left;
  height: 38px;
  line-height: 38px;
}
#wrapper #scroller .cabbage_tab ul li a {
  margin: 0 12px;
  text-decoration: none;
}
#wrapper #scroller .cabbage_tab .active {
  border-bottom: 2px solid red;
}
#wrapper #scroller .cabbage_tab .active a {
  color: red;
}
/*白菜价tab end---------------------*/
/*白菜价body部分  begin--------------- */
.body {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.body .cabbage_body {
  border-top: 1px solid #ccc;
  padding: 8px 0px;
}
.body .cabbage_body .items {
  width: 100%;
}
.body .cabbage_body .items ul li {
  height: 140px;
  margin-bottom: 10px;
  background-color: #fff;
  padding: 5px 5px;
  position: relative;
  transition: all .5s;
}
.body .cabbage_body .items ul li i {
  width: 0;
  height: 0;
  position: absolute;
  top: 50%;
  left: 50%;
  transition: all .5s;
}
.body .cabbage_body .items ul li .one {
  background: linear-gradient(to right, deepskyblue 10%, transparent 10%, transparent 90%, deepskyblue 90%);
}
.body .cabbage_body .items ul li .two {
  background: linear-gradient(to bottom, deepskyblue 20%, transparent 20%, transparent 80%, deepskyblue 80%);
}
.body .cabbage_body .items ul li .three {
  background: linear-gradient(to right, deepskyblue 10%, transparent 10%, transparent 90%, deepskyblue 90%);
}
.body .cabbage_body .items ul li .four {
  background: linear-gradient(to bottom, deepskyblue 20%, transparent 20%, transparent 80%, deepskyblue 80%);
}
.body .cabbage_body .items ul li:hover {
  transform: scale(0.9);
  box-shadow: 0 0 40px deeppink;
}
.body .cabbage_body .items ul li:hover .one {
  width: 108%;
  height: 3px;
  top: -5%;
  left: -4%;
}
.body .cabbage_body .items ul li:hover .two {
  width: 3px;
  height: 112%;
  top: -5%;
  left: 104%;
}
.body .cabbage_body .items ul li:hover .three {
  width: 108%;
  height: 3px;
  top: 105%;
  left: -4%;
}
.body .cabbage_body .items ul li:hover .four {
  width: 3px;
  height: 108%;
  top: -4%;
  left: -4%;
}
.body .cabbage_body .items .picture {
  width: 130px;
  height: 130px;
  margin-right: 5px;
  float: left;
}
.body .cabbage_body .items .picture img {
  width: 100%;
  height: 100%;
}
.body .cabbage_body .items .right .top {
  margin-bottom: 5px;
}
.body .cabbage_body .items .right .top div {
  color: red;
}
.body .cabbage_body .items .right .top div:nth-child(1) {
  display: block;
  margin-bottom: 5px;
  color: #000;
  height: 40px;
}
.body .cabbage_body .items .right .top div:nth-child(2) {
  font-size: 20px;
  padding-left: 180px;
}
.body .cabbage_body .items .right .top div:nth-child(2) i,
.body .cabbage_body .items .right .top div:nth-child(2) del {
  font-style: normal;
  font-size: 14px;
  margin-right: 5px;
}
.body .cabbage_body .items .right .top div:nth-child(2) i {
  width: 30px;
  top: 40%;
  left: 35%;
}
.body .cabbage_body .items .right .top div:nth-child(2) em {
  font-style: normal;
  font-size: 12px;
}
.body .cabbage_body .items .right .top div:nth-child(2) del {
  margin-left: 10px;
  color: #999;
}
.body .cabbage_body .items .right .bottom {
  position: relative;
  height: 50px;
  margin-top: 5px;
}
.body .cabbage_body .items .right .bottom .bar {
  width: 80px;
  position: relative;
}
.body .cabbage_body .items .right .bottom .bar i {
  position: absolute;
  left: 135px;
  top: 0;
  width: 80px;
  height: 15px;
  line-height: 15px;
  background-color: #ffb7b7;
  border-radius: 20px;
  overflow: hidden;
}
.body .cabbage_body .items .right .bottom .bar i em {
  height: 100%;
  border-radius: 15px;
  position: absolute;
  top: 0;
  left: 0;
  background-color: red;
  border: 1px solid red;
}
.body .cabbage_body .items .right .bottom .bar i span {
  position: absolute;
  left: 27px;
  color: white;
  font-style: normal;
  font-weight: bold;
  font-size: 12px;
}
.body .cabbage_body .items .right .bottom .bar b {
  color: red;
  font-weight: normal;
  font-size: 12px;
  position: absolute;
  right: -220px;
}
.body .cabbage_body .items .right .bottom .discount {
  position: absolute;
  left: 135px;
  bottom: 0;
  width: 120px;
  height: 25px;
  line-height: 25px;
  text-align: center;
  font-size: 12px;
  border: 1px dashed red;
  color: red;
  margin-top: 10px;
}
.body .cabbage_body .items .right .bottom .productHref {
  width: 70px;
  height: 25px;
  line-height: 25px;
  text-align: center;
  border-radius: 5px;
  background-color: #ff5e5d;
  color: white;
  position: absolute;
  right: 0;
  bottom: 0px;
}
/*白菜价body部分  end--------------- */
/*白菜价foot部分  begin--------------- */
/*底部 部分*/
.footerApp {
  padding-top: 25px;
  background-color: #f2f2f2;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
}
.footerApp .foot_top {
  background: #efefef;
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
  height: 34px;
  line-height: 34px;
}
.footerApp .foot_top a {
  width: 30%;
  float: left;
  text-align: center;
  font-size: 14px;
  border-right: 1px solid #c4c4c4;
}
.footerApp .foot_top a img {
  width: 15px;
  height: 15px;
  border: none;
  display: inline-block;
  vertical-align: middle;
  margin-top: -5px;
  margin-right: 2px;
}
.footerApp .foot_top a:last-child {
  width: 40%;
  border-right: none;
}
.footerApp .footer {
  font-size: 12px;
  color: #666;
  padding: 15px 0;
}
.footerApp .footer p {
  text-align: center;
}
.footerApp .footer p a {
  display: inline;
}
.footerApp .footer p a:first-child {
  color: red;
}
.footerApp .footer p a:last-child {
  color: #000;
}
/*白菜价foot部分  end--------------- */
