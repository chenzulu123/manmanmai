/* 担心有些ie浏览器 不识别 媒体查询 only   */
@media only screen and (max-width: 1080px), only screen and (max-device-width: 1080px) {
  html,
  body {
    font-size: 168.75px;
  }
}
@media only screen and (max-width: 960px), only screen and (max-device-width: 960px) {
  html,
  body {
    font-size: 150px;
  }
}
@media only screen and (max-width: 800px), only screen and (max-device-width: 800px) {
  html,
  body {
    font-size: 125px;
  }
}
@media only screen and (max-width: 720px), only screen and (max-device-width: 720px) {
  html,
  body {
    font-size: 112.5px;
  }
}
@media only screen and (max-width: 640px), only screen and (max-device-width: 640px) {
  html,
  body {
    font-size: 100px;
  }
}
@media only screen and (max-width: 600px), only screen and (max-device-width: 600px) {
  html,
  body {
    font-size: 93.75px;
  }
}
@media only screen and (max-width: 540px), only screen and (max-device-width: 540px) {
  html,
  body {
    font-size: 84.375px;
  }
}
@media only screen and (max-width: 480px), only screen and (max-device-width: 480px) {
  html,
  body {
    font-size: 75px;
  }
}
@media only screen and (max-width: 414px), only screen and (max-device-width: 414px) {
  html,
  body {
    font-size: 64.6875px;
  }
}
@media only screen and (max-width: 400px), only screen and (max-device-width: 400px) {
  html,
  body {
    font-size: 62.5px;
  }
}
@media only screen and (max-width: 375px), only screen and (max-device-width: 375px) {
  html,
  body {
    font-size: 58.59375px;
  }
}
@media only screen and (max-width: 360px), only screen and (max-device-width: 360px) {
  html,
  body {
    font-size: 56.25px;
  }
}
@media only screen and (max-width: 320px), only screen and (max-device-width: 320px) {
  html,
  body {
    font-size: 50px;
  }
}
@media only screen and (max-width: 240px), only screen and (max-device-width: 240px) {
  html,
  body {
    font-size: 37.5px;
  }
}
@media screen and (width: 6.4rem) {
  html {
    font-size: 1rem;
  }
}
html,
body {
  height: 100%;
}
* {
  margin: 0;
  padding: 0;
  -webkit-tap-highlight-color: transparent;
}
a {
  display: block;
  text-decoration: none;
  color: inherit;
}
.clearfix:after {
  display: block;
  content: "";
  height: 0;
  clear: both;
}
img {
  width: 100px;
  display: block;
}
input {
  outline: none;
  border: none;
}
/*头部开始*/
.head {
  width: 100%;
  height: 0.44rem;
  color: #fff;
  background: #ff841d;
}
.head .back {
  float: left;
  margin-left: 0.1rem;
  width: 0.5rem;
  height: 0.44rem;
  line-height: 0.44rem;
}
.head .back a {
  font-size: 0.3rem;
}
.head span {
  float: left;
  width: 5.2rem;
  height: 0.44rem;
  font-size: 0.2rem;
  font-weight: normal;
  text-align: center;
  line-height: 0.44rem;
  margin: 0 auto;
}
.head .home {
  float: right;
  margin-right: 0.1rem;
  width: 0.5rem;
  height: 0.5rem;
}
.head .home img {
  display: block;
  width: 0.4rem;
  height: 0.4rem;
  margin: 0.02rem auto;
}
/*头部结束*/
/*内容开始*/
.container .cu_ontent {
  padding: 0.04rem 0.1rem;
}
.container .cu_ontent h3 {
  font-size: 0.18rem;
  margin: 0.1rem auto 0.05rem auto;
  text-align: left;
  color: #5a5a5a;
}
.container .cu_ontent h4 {
  font-size: 0.15rem;
  color: #ff6c00;
  font-weight: bold;
}
.container .cu_ontent .info {
  margin: 0.12rem auto;
  color: #999999;
  font-size: 0.12rem;
  line-height: 0.24rem;
}
.container .cu_ontent .info .mall {
  margin: 0 0.04rem;
}
.container .cu_ontent .info .addtime {
  margin-right: 0.04rem;
}
.container .cu_ontent .content .conn {
  width: 5rem;
  float: left;
  font-size: 0.16rem;
  line-height: 0.24rem;
  color: #606060;
  padding-bottom: 0.2rem;
}
.container .cu_ontent .content .conn p {
  margin: 0.06rem auto;
}
.container .cu_ontent .content .conn a {
  color: #4682c5;
  display: inline;
}
.container .cu_ontent .content .picwrap {
  width: 1rem;
  height: 1rem;
  float: right;
  margin: 0.12rem auto auto 0.1rem;
}
.container .cu_ontent .content .picwrap img {
  width: 1rem;
  height: 1rem;
}
.container .cu_ontent .golink {
  width: 100%;
}
.container .cu_ontent .golink a {
  display: block;
  height: 0.4rem;
  line-height: 0.4rem;
  margin: 0.2rem 0.1rem;
  background: #FF9000;
  text-align: center;
  color: #fff;
  font-size: 0.14rem;
  border-radius: 0.04rem;
}
.container .comment {
  border-top: 1px solid #e0e0e0;
}
.container .comment .cu-content-pl .tit {
  padding: 0.06rem 0.08rem;
  font-size: 0.16rem;
}
.container .comment .cu-content-pl .reply {
  padding: 0.06rem 0.08rem;
}
.container .comment .cu-content-pl .reply .form {
  position: relative;
  border: 2px solid #ECEAE8;
  border-radius: 2px;
  padding: 4px 0 0 4px;
}
.container .comment .cu-content-pl .reply .form textarea {
  width: 100%;
  height: 0.68rem;
  resize: none;
  outline: none;
  border: none;
  overflow-x: hidden;
  overflow-y: auto;
  line-height: 0.22rem;
  color: #666;
}
.container .comment .cu-content-pl .reply .form .ctrl {
  height: 0.36rem;
  background: #fbfbfb;
  border-top: 1px solid #f2f2f2;
  margin-left: -4px;
}
.container .comment .cu-content-pl .reply .form .ctrl input {
  width: 1rem;
  height: 0.39rem;
  position: absolute;
  right: -1px;
  bottom: -2px;
  color: #fff;
  outline: none;
  font-size: 0.16rem;
  background: #ff841d;
}
.container .comment .cu-content-pl .list ul {
  list-style: none;
}
.container .comment .cu-content-pl .list ul li {
  width: 6.2rem;
  padding: 0.18rem 0.1rem;
  border-bottom: 1px solid #e0e0e0;
}
.container .comment .cu-content-pl .list ul li .userimg {
  float: left;
  width: 0.4rem;
  height: 0.4rem;
  border-radius: 50%;
  overflow: hidden;
}
.container .comment .cu-content-pl .list ul li .userimg img {
  width: 0.4rem;
  height: 0.4rem;
}
.container .comment .cu-content-pl .list ul li .con {
  font-size: 0.14rem;
  float: right;
}
.container .comment .cu-content-pl .list ul li .con .name {
  width: 5.7rem;
  height: 0.15rem;
  font-size: 0.12rem;
}
.container .comment .cu-content-pl .list ul li .con .name .username {
  color: #5799cc;
  float: left;
}
.container .comment .cu-content-pl .list ul li .con .name .time {
  float: right;
  color: #c3c3c3;
}
.container .comment .cu-content-pl .list ul li .con .content {
  width: 5.7rem;
  float: left;
  padding-top: 0.02rem;
  font-size: 0.14rem;
  line-height: 0.22rem;
  color: #696969;
}
.container .comment .cu-content-pl .list ul li .con .content img {
  display: inline-block;
  width: 0.22rem;
  height: 0.22rem;
}
.container .comment .cu-content-pl .list ul li .con .content .yinyong {
  background-color: #fbfbfb;
  padding-top: 0.04rem 0.05rem 0 0.05rem;
  margin-bottom: 0.1rem;
  border-radius: 0.05rem;
  margin: 0 0.1rem;
  border: 1px solid #eeeeee;
}
.container .comment .cu-content-pl .list ul li .con .content .yinyong img {
  display: inline-block;
  width: 0.22rem;
  height: 0.22rem;
}
.container .comment .cu-content-pl .list ul li .con .content .yinyong span {
  color: #5799cc;
  padding: 0.04rem 0.06rem;
  font-size: 0.14rem;
  height: 0.2rem;
  line-height: 0.2rem;
}
.container .comment .cu-content-pl .list ul li:after {
  display: block;
  content: "";
  height: 0;
  clear: both;
}
/*内容结束*/
/*导航栏开始*/
.nav {
  width: 6.32rem;
  height: 0.36rem;
  line-height: 0.36rem;
  padding-left: 0.08rem;
  font-size: 0.14rem;
  margin-top: 0.2rem;
  background: #ebebeb;
  background: -webkit-gradient(linear, 0 0, 0 100%, from(#ffffff), to(#ebebeb));
  border-top: 1px solid #ebebeb;
  border-bottom: 1px solid #ccc;
}
.nav a {
  display: inline-block;
}
/*导航栏结束*/
/*底部开始*/
.footerApp {
  margin-top: 0.25rem;
}
.footerApp .foot_top {
  height: 0.34rem;
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
  line-height: 0.34rem;
  background: #efefef;
}
.footerApp .foot_top a {
  width: 30%;
  float: left;
  box-sizing: border-box;
  text-align: center;
  font-size: 14px;
  border-right: 1px solid #c4c4c4;
}
.footerApp .foot_top a:last-child {
  width: 40%;
  border: 0;
  text-align: center;
}
.footerApp .foot_top a:last-child img {
  width: 0.15rem;
  height: 0.15rem;
  display: inline-block;
  vertical-align: middle;
  margin-top: -5px;
  margin-right: -2px;
}
.footerApp .footer {
  background: #efefef;
  text-align: center;
  font-size: 0.12rem;
  padding: 0.15rem 0;
  color: #666;
}
.footerApp .footer a {
  display: inline-block;
}
.footerApp .footer a:nth-child(1) {
  color: red;
}
.footerApp .footer a:nth-child(2) {
  color: #000;
}
/*底部结束*/
/*App下载开始*/
.fixed-slow {
  width: 0.6rem;
  height: 0.67rem;
  position: fixed;
  right: 0px;
  bottom: 0.3rem;
  z-index: 1;
}
.fixed-slow a {
  width: 0.6rem;
  height: 0.67rem;
  background: url('../images/slow.png') no-repeat center / 0.6rem 0.67rem;
}
/*App下载结束*/
