* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
a {
  text-decoration: none;
  color: inherit;
  display: block;
  width: 100%;
  height: 100%;
  /* 解决点击高亮*/
  -webkit-tap-highlight-color: transparent;
}
input {
  display: block;
  width: 100%;
  height: 100%;
  border: none;
}
img {
  display: block;
  width: 100%;
}
ul {
  list-style: none;
}
.clearfix:after {
  display: block;
  content: "";
  height: 0;
  clear: both;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
.loading {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  background: gray;
}
.loading .pic {
  /*要定位的元素自动居中，可以先设置四个方位的距离都为0，然后直接来一个margin：auto  */
  width: 50px;
  height: 50px;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto;
}
.loading .pic i {
  float: left;
  display: block;
  width: 6px;
  height: 50px;
  margin: 0 2px;
  background: #399;
  -webkit-transform: scaleY(0.4);
  -ms-transform: scaleY(0.4);
  transform: scaleY(0.4);
  /*设置i的高度进行变化，变化时间是1.2s间隔，循环播放*/
  -webkit-animation: load 1.2s infinite;
  animation: load 1.2s infinite;
}
@-webkit-keyframes load {
  0%,
  40%,
  100% {
    /*0%,40%,100%进行0.4的缩放  */
    -webkit-transform: scaleY(0.4);
    transform: scaleY(0.4);
  }
  /*20%进行100%的缩放  */
  20% {
    -webkit-transform: scaleY(1);
    transform: scaleY(1);
  }
}
@keyframes load {
  0%,
  40%,
  100% {
    /*0%,40%,100%进行0.4的缩放  */
    -webkit-transform: scaleY(0.4);
    transform: scaleY(0.4);
  }
  /*20%进行100%的缩放  */
  20% {
    -webkit-transform: scaleY(1);
    transform: scaleY(1);
  }
}
div,
a,
img {
  -webkit-tap-highlight-color: transparent;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  user-select: none;
}
/*给单个的i标签设置延迟时间*/
.loading .pic i:nth-child(2) {
  -webkit-animation-delay: 0.1s;
  animation-delay: 0.1s;
}
.loading .pic i:nth-child(3) {
  -webkit-animation-delay: 0.2s;
  animation-delay: 0.2s;
}
.loading .pic i:nth-child(4) {
  -webkit-animation-delay: 0.3s;
  animation-delay: 0.3s;
}
.loading .pic i:nth-child(5) {
  -webkit-animation-delay: 0.4s;
  animation-delay: 0.4s;
}
.view .header {
  width: 6.4rem;
  padding: 0.15rem 0.18rem 0.15rem 0.18rem;
  background-color: #ff841d;
}
.view .header .logo {
  float: left;
  width: 3.26rem;
  height: 0.45rem;
}
.view .header .down_button {
  float: right;
  width: 0.45rem;
  height: 0.45rem;
}
.view .search {
  padding: 0.15rem;
  background-color: #ffffff;
}
.view .search > .searchBox {
  width: 5.1rem;
  border-radius: 0.25rem;
  box-sizing: border-box;
  padding-left: 0.25rem;
  height: 0.5rem;
  color: #757575;
  float: left;
  background-color: #f1f1f1;
  border: none;
  font-size: 0.2rem;
  outline: none;
}
.view .search > .search_btn {
  width: 0.9rem;
  height: 0.5rem;
  background-color: #ff841d;
  color: #ffffff;
  float: right;
  border-radius: 0.25rem;
  padding: 0 0.2rem 0 0.2rem;
  font-size: 0.2rem;
  outline: none;
}
.view .search > .search_btn:hover {
  background-color: orangered;
}
.view .line {
  width: 100%;
  height: 0.05rem;
  background-color: #f1f1f1;
}
.view .menu_bar {
  padding-bottom: 0.16rem;
}
.view .menu_bar ul {
  list-style: none;
}
.view .menu_bar ul li {
  margin-top: 0.16rem;
  display: block;
  text-align: center;
  height: 0.9rem;
  float: left;
  width: 1.6rem;
}
.view .menu_bar ul li a img {
  margin: 0 auto;
  width: 0.6rem;
  height: 0.6rem;
}
.view .menu_bar ul li a img:hover {
  transform: rotateY(360deg);
  transition: all .5s;
}
.view .menu_bar ul li a span {
  margin-top: 0.06rem;
  display: block;
  height: 0.2rem;
  font-size: 0.14rem;
  color: #767676;
}
.view .menu_bar ul li:nth-child(n+9) {
  display: none;
}
.view .discount_recommend,
.view .brand_among {
  background-color: #ff9000;
  height: 0.4rem;
  line-height: 0.4rem;
  padding-left: 0.1rem;
  vertical-align: middle;
}
.view .discount_recommend span,
.view .brand_among span {
  float: left;
  font-size: 0.2rem;
  color: #ffffff;
}
.view .discount_recommend img,
.view .brand_among img {
  margin-top: 0.07rem;
  width: 0.26rem;
  height: 0.26rem;
  float: right;
  margin-right: 0.2rem;
  vertical-align: middle;
}
.view .discount_content .discountList {
  list-style: none;
}
.view .discount_content .discountList li {
  padding: 0.14rem 0.08rem;
  float: left;
  position: relative;
  overflow: hidden;
  transition: all .5s;
}
.view .discount_content .discountList li:hover {
  transform: scale(0.9);
  box-shadow: 0 0 20px lightslategray;
  border-radius: 10px;
  background-color: lightcyan;
}
.view .discount_content .discountList li a {
  width: 100%;
  height: 100%;
}
.view .discount_content .discountList li a img {
  width: 1rem;
  height: 1rem;
  float: left;
}
.view .discount_content .discountList li a .info {
  margin-left: 0.24rem;
  width: 5rem;
  float: right;
  font-size: 0.14rem;
}
.view .discount_content .discountList li a .info .title {
  width: 4rem;
  height: 0.57rem;
  color: #333333;
}
.view .discount_content .discountList li a .info .title span {
