}
.view .discount_content .discountList li a .info .other {
  width: 100%;
  height: 0.24rem;
  font-size: 0.12rem;
  color: #cfcfcf;
}
.view .discount_content .discountList li a .info .other .mall {
  display: block;
  float: left;
}
.view .discount_content .discountList li a .info .other .icon_number {
  float: right;
  margin-right: 0.4rem;
}
.view .discount_content .discountList li a .info .other .icon_number .iconfont {
