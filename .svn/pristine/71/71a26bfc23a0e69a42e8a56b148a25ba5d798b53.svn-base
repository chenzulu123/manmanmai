}
.view .discount_content .more_info {
  height: 0.8rem;
  background-color: #f2f2f2;
}
.view .discount_content .more_info .more_discount {
  padding: 0.1rem 0.1rem 0 0.1rem;
}
.view .discount_content .more_info .more_discount a {
  display: block;
  width: 6.2rem;
  height: 0.4rem;
  border-radius: 0.05rem;
  text-align: center;
  border: 1px solid #dddddd;
  box-sizing: border-box;
  background-color: #ffffff;
  line-height: 0.4rem;
  font-size: 0.14rem;
  color: #767676;
}
.view .footer {
  width: 100%;
}
.view .footer ul {
  margin-top: 0.15rem;
  list-style: none;
  margin-bottom: 0.2rem;
}
.view .footer ul li {
  float: left;
  width: 33.333%;
  border: 1px solid #c4c4c4;
  box-sizing: border-box;
  text-align: center;
  height: 0.4rem;
}
.view .footer ul li a {
  display: block;
  width: 100%;
  font-size: 0.14rem;
  line-height: 0.4rem;
  color: #767676;
}
.view .footer ul li:nth-child(1) {
  border-left: none;
  border-right: none;
}
.view .footer ul li:nth-child(3) {
  border-left: none;
  border-right: none;
}
.view .footer p {
  text-align: center;
  font-size: 0.14rem;
  color: #9f9f9f;
}
.view .footer p a {
  display: inline-block;
  color: #fd2929;
}
