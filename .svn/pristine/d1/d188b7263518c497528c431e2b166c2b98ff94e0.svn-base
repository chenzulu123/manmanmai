<!--模板-->
<script type="text/html" id="template">
    <div class="cu_ontent">
        <h3>{{productName}}</h3>
        <h4>{{productPinkage}}</h4>
        <div class="info">
            <span class="mall">{{productFrom}}</span>
            <span class="addtime">{{productTime}}</span>
            <span class="author">{{productTips}}</span>
        </div>
        <div class="content clearfix">
            <div class="picwrap">
                {{@productImgSm}}
            </div>
            <div class="conn">
                <p>
                    促销类型:&nbsp;[用券]
                    <a href="javascript:void(0)">50.00元满减券</a>
                </p>
                <p>{{productInfo}}</p>
            </div>
        </div>
        <div class="golink">
            <a href="javascript:void(0)">前往购买</a>
        </div>
    </div>
    <div class="comment">{{@productComment}}</div>
</script>
<!--模板2-->
<script type="text/html" id="template1">
