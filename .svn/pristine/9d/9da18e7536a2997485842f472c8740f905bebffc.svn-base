* {
  margin: 0;
  padding: 0;
}
a {
  text-decoration: none;
}
ul {
  list-style: none;
}
.clearfix::after {
  display: block;
  content: "";
  height: 0;
  clear: both;
}
header {
  width: 100%;
  height: 45px;
  border-radius: 3px;
  background-color: #f4483e;
  position: fixed;
}
header .skip {
  height: 45px;
  width: 30px;
  display: block;
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 7px;
}
header p {
  font-size: 21px;
  color: white;
  text-align: center;
  line-height: 45px;
}
header a {
  display: block;
  position: absolute;
  top: 0;
  right: 13px;
}
header a img {
  margin-top: 3px;
  width: 35px;
  height: 35px;
}
main {
  padding-top: 45px;
  width: 100%;
  border: 1px solid #eeeeee;
}
main ul {
  margin-top: 5px;
  height: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
main ul li {
  width: 32%;
  border-radius: 10px;
  box-shadow: 0 2px 3px gray;
}
main ul li a img {
  display: block;
  width: 32%;
  margin: 0 auto;
}
main ul li a p {
  font-size: 14px;
  text-align: center;
  color: black;
}
main ul li:last-child {
  margin-top: 5px;
  margin-left: 1.333%;
}
.mid {
  width: 100%;
  background-color: #f2f2f2;
  box-shadow: 1px 1px 2px #cccccc;
  font-size: 16px;
  line-height: 35px;
  padding-left: 10px;
}
.mid a {
  color: #000000;
}
.gap {
  height: 25px;
  border-radius: 5px;
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
  background: linear-gradient(180deg, red 25%, white 25%, white 50%, blue 50%, blue 75%, red 75%) bottom / 50px 100px;
}
footer {
  width: 100%;
  background-color: #efefef;
  padding-bottom: 15px;
}
footer ul {
  width: 100%;
}
footer ul li {
  float: left;
  line-height: 35px;
  text-align: center;
  border-radius: 5px;
  width: 30%;
  border-top: 1px solid #c4c4c4;
  border-right: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
}
footer ul li a {
  color: #000000;
}
footer ul li:last-child {
  width: 40%;
  border-right: none;
}
footer ul li:last-child img {
  width: 16px;
  height: 16px;
}
footer .tool {
  text-align: center;
}
footer .tool .red {
  color: #ff0000;
}
footer .tool .gray {
  color: #666666;
}
footer p {
  text-align: center;
  color: #666666;
}
