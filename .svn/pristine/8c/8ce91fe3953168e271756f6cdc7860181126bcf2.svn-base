* {
  margin: 0;
  padding: 0;
}
a {
  text-decoration: none;
}
ul {
  list-style: none;
}
.clearfix::after {
  display: block;
  content: "";
  height: 0;
  clear: both;
}
header {
  z-index: 1;
  border-radius: 3px;
  width: 100%;
  height: 45px;
  background-color: #f4483e;
  position: fixed;
}
header .skip {
  height: 30px;
  width: 30px;
  display: block;
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 7px;
}
header p {
  font-size: 21px;
  color: white;
  text-align: center;
  line-height: 45px;
}
header a {
  display: block;
  position: absolute;
  top: 0;
  right: 13px;
}
header a img {
  margin-top: 3px;
  width: 35px;
  height: 35px;
}
.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0px;
  z-index: 1002;
  left: 0px;
  background-color: rgba(0, 0, 0, 0.5);
}
.mask .swiper-container {
  width: 300px;
  height: 300px;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -150px;
  margin-top: -150px;
}
.mask .swiper-container .swiper-button-prev {
  left: -60px;
}
.mask .swiper-container .swiper-button-next {
  right: -60px;
}
.mask .swiper-slide {
  background-position: center;
  background-size: cover;
}
.mask .swiper-slide img {
  width: 100%;
  height: 100%;
}
.prompt {
  width: 100%;
  padding-top: 50px;
  padding-bottom: 16px;
  text-align: center;
}
.model-out {
  padding-bottom: 20px;
  width: 100%;
  border-bottom: 1px solid #e7e7e7;
  position: relative;
}
.model-out a img {
  position: absolute;
  width: 40px;
  height: 40px;
  right: 0;
  top: 40%;
}
.model-out .up {
  width: 100%;
}
.model-out .up .left {
  float: left;
  width: 33.33%;
}
.model-out .up .left img {
  width: 100%;
  border-radius: 5px;
  box-shadow: 1px 3px 5px gray;
}
.model-out .up .right {
  padding-left: 1%;
  padding-right: 2%;
  float: left;
  width: 63.66%;
}
.model-out .up .right p {
  font-size: 16px;
  line-height: 30px;
}
.model-out .up .right span {
  font-weight: 700;
}
.model-out .down p {
  text-align: center;
  color: #ee7700;
}
.loading {
  display: none;
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.5);
}
.loading .pic {
  /*要定位的元素自动居中，可以先设置四个方位的距离都为0，然后直接来一个margin：auto  */
  width: 50px;
  height: 50px;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto;
}
.loading .pic i {
  float: left;
  display: block;
  width: 6px;
  height: 50px;
  margin: 0 2px;
  background: #399;
  -webkit-transform: scaleY(0.4);
  -ms-transform: scaleY(0.4);
  transform: scaleY(0.4);
  /*设置i的高度进行变化，变化时间是1.2s间隔，循环播放*/
  -webkit-animation: load 1.2s infinite;
  animation: load 1.2s infinite;
}
@-webkit-keyframes load {
  0%,
  40%,
  100% {
    /*0%,40%,100%进行0.4的缩放  */
    -webkit-transform: scaleY(0.4);
    transform: scaleY(0.4);
  }
  /*20%进行100%的缩放  */
  20% {
    -webkit-transform: scaleY(1);
    transform: scaleY(1);
  }
}
@keyframes load {
  0%,
  40%,
  100% {
    /*0%,40%,100%进行0.4的缩放  */
    -webkit-transform: scaleY(0.4);
    transform: scaleY(0.4);
  }
  /*20%进行100%的缩放  */
  20% {
    -webkit-transform: scaleY(1);
    transform: scaleY(1);
  }
}
/*给单个的i标签设置延迟时间*/
.loading .pic i:nth-child(2) {
  -webkit-animation-delay: 0.1s;
  animation-delay: 0.1s;
}
.loading .pic i:nth-child(3) {
  -webkit-animation-delay: 0.2s;
  animation-delay: 0.2s;
}
.loading .pic i:nth-child(4) {
  -webkit-animation-delay: 0.3s;
  animation-delay: 0.3s;
}
.loading .pic i:nth-child(5) {
  -webkit-animation-delay: 0.4s;
  animation-delay: 0.4s;
}
#backtop {
  display: none;
  position: fixed;
  bottom: 0;
  right: 0;
  cursor: pointer;
  border: 1px solid #ccc;
  border-radius: 5px;
  background-color: skyblue;
}
