* {
  margin: 0;
  padding: 0;
  /* 盒子模型的 怪异模式  */
}
html {
  -webkit-font-smoothing: antialiased;
}
body {
  font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif !important;
}
a {
  text-decoration: none;
  color: inherit;
  display: block;
  /* 移动端 a标签 点击高亮 */
  -webkit-tap-highlight-color: transparent;
}
ul {
  list-style: none;
}
img {
  width: 100%;
  /* 解决图片默认的3像素  */
  display: block;
}
input {
  display: block;
  width: 100%;
  height: 100%;
  border: none;
}
/* 清除浮动 */
.clearfix::after {
  display: block;
  content: "";
  height: 0;
  clear: both;
}
@font-face {
  font-family: 'iconfont';
  src: url('../font/font_q0dku14gt5gkfbt9/iconfont.eot');
  src: url('../font/font_q0dku14gt5gkfbt9/iconfont.eot?#iefix') format('embedded-opentype'), url('../font/font_q0dku14gt5gkfbt9/iconfont.woff') format('woff'), url('../font/font_q0dku14gt5gkfbt9/iconfont.ttf') format('truetype'), url('../font/font_q0dku14gt5gkfbt9/iconfont.svg#iconfont') format('svg');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 18px;
  font-style: normal;
  font-weight: bold;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
/*头部开始*/
.head {
  width: 100%;
  height: 44px;
  color: #fff;
  background: #ff841d;
  position: relative;
}
.head .back {
  position: absolute;
  left: 10px;
  width: 50px;
  height: 44px;
  line-height: 44px;
}
.head h1 {
  height: 44px;
  font-size: 20px;
  font-weight: normal;
  text-align: center;
  margin: 0;
  line-height: 44px;
}
.head .home {
  position: absolute;
  right: 10px;
  bottom: 0;
  width: 50px;
  height: 44px;
  background: url('../images/header_app.png') no-repeat center / 32px;
}
.head .home a {
  height: 44px;
}
/*头部结束*/
/*内容开始*/
.cu-friend ul {
  padding: 0 0 10px 0;
}
.cu-friend li {
  width: 50%;
  float: left;
  box-sizing: border-box;
  border: 1px solid #eee;
  font-size: 14px;
}
.cu-friend li .img {
  margin: 15px auto;
  width: 140px;
  height: 140px;
}
.cu-friend li .img img {
  width: 140px;
  height: 140px;
}
.cu-friend li .title {
  height: 36px;
  padding-left: 18px;
  padding-right: 8px;
  line-height: 18px;
  text-align: left;
  font-weight: bold;
  color: #333;
  word-break: break-all;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.cu-friend li .subtit {
  height: 18px;
  margin-top: 4px;
  padding-left: 18px;
  padding-right: 8px;
  color: #ff6c00;
  line-height: 18px;
  text-align: left;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.cu-friend li .info {
  padding: 6px 8px 6px 18px;
  line-height: 24px;
  color: #a1a1a1;
  font-size: 12px;
  text-align: left;
}
/*内容结束*/
/*导航栏开始*/
.nav {
  width: 100%;
  height: 36px;
  line-height: 36px;
  padding-left: 8px;
  font-size: 14px;
  margin-top: 20px;
  background: #ebebeb;
  background: -webkit-gradient(linear, 0 0, 0 100%, from(#ffffff), to(#ebebeb));
  border-top: 1px solid #ebebeb;
  border-bottom: 1px solid #ccc;
}
.nav a {
  display: inline-block;
}
/*导航栏结束*/
/*底部开始*/
.footerApp {
  margin-top: 25px;
}
.footerApp .foot_top {
  height: 34px;
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
  line-height: 34px;
  background: #efefef;
}
.footerApp .foot_top a {
  width: 30%;
  float: left;
  box-sizing: border-box;
  text-align: center;
  font-size: 14px;
  border-right: 1px solid #c4c4c4;
}
.footerApp .foot_top a:last-child {
  width: 40%;
  border: 0;
  text-align: center;
}
.footerApp .foot_top a:last-child img {
  width: 15px;
  height: 15px;
  display: inline-block;
  vertical-align: middle;
  margin-top: -5px;
  margin-right: -2px;
}
.footerApp .footer {
  background: #efefef;
  text-align: center;
  font-size: 12px;
  padding: 15px 0;
  color: #666;
}
.footerApp .footer a {
  display: inline-block;
}
.footerApp .footer a:nth-child(1) {
  color: red;
}
.footerApp .footer a:nth-child(2) {
  color: #000;
}
/*底部结束*/
/*App下载开始*/
.fixed-slow {
  width: 60px;
  height: 67px;
  position: fixed;
  right: 0px;
  bottom: 30px;
  z-index: 999;
}
.fixed-slow a {
  width: 60px;
  height: 67px;
  background: url('../images/slow.png') no-repeat center / 60px 67px;
}
/*App下载结束*/
