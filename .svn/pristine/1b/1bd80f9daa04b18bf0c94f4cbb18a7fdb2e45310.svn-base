* {
  margin: 0;
  padding: 0;
  /* 盒子模型的 怪异模式  */
}
html {
  -webkit-font-smoothing: antialiased;
}
body {
  font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif !important;
}
a {
  text-decoration: none;
  color: inherit;
  display: block;
  /* 移动端 a标签 点击高亮 */
  -webkit-tap-highlight-color: transparent;
}
ul {
  list-style: none;
}
img {
  width: 100%;
  /* 解决图片默认的3像素  */
  display: block;
}
input {
  display: block;
  width: 100%;
  height: 100%;
  border: none;
}
/* 清除浮动 */
.clearfix::after {
  display: block;
  content: "";
  height: 0;
  clear: both;
}
@font-face {
  font-family: 'iconfont';
  src: url('../font/font_q0dku14gt5gkfbt9/iconfont.eot');
  src: url('../font/font_q0dku14gt5gkfbt9/iconfont.eot?#iefix') format('embedded-opentype'), url('../font/font_q0dku14gt5gkfbt9/iconfont.woff') format('woff'), url('../font/font_q0dku14gt5gkfbt9/iconfont.ttf') format('truetype'), url('../font/font_q0dku14gt5gkfbt9/iconfont.svg#iconfont') format('svg');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 18px;
  font-style: normal;
  font-weight: bold;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
